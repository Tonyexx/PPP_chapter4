// exercise_7.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "stdafx.h"
#include<iostream>
#include<string>
#include<vector>
#include<algorithm>
#include<cmath>
	using namespace std;
	inline void keep_window_open() { char ch; cin >> ch; }


	int main()
	{		
		char let = 'a';  //let means letter
		int num = 97;
		while (let <= 'z')
		{
			cout << let << "\t" << num<<"\n";
			++let;
			++num;
		}
		keep_window_open();
	}
